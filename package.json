{
  "name": "discloud-deploy-action",
  "description": "This action deploys your application to Discloud",
  "version": "1.1.0",
  "main": "dist",
  "license": "Apache-2.0",
  "scripts": {
    "build:test": "tsc --outDir out",
    "package": "ncc build src/index.ts -ms",
    "test": "npm run test:ts && npm run test:lint && npm run build:test && npm run test:node",
    "test:node": "node --test \"**/test/**/*.test.?(c|m)js\"",
    "test:lint": "eslint .",
    "test:ts": "tsc --noEmit",
    "watch": "tsc -w --noEmit"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@discloudapp/api-types": "^0.7.3",
    "@discloudapp/util": "^0.10.1",
    "bytes": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/bytes": "^3.1.5",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "typescript": "^5.8.3"
  }
}
